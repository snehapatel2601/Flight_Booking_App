<mvc:View controllerName="user.user.controller.home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
       <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop img"
	             backgroundImage="data:image/jpeg;base64,"
	             frameType="OneByOne" press="enterBookFlight">
		<TileContent footer="Flights" frameType="OneByOne">
			<NewsContent
					contentText="Book Flights"
					subheader="International/Domestic"
                     />
		</TileContent>
	</GenericTile>
        <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
        header="Booking History" 
        subheader="View Booked Flights" 
        press="enterHistory" >
        <TileContent>
            <ImageContent src="sap-icon://history"></ImageContent>
            
        </TileContent>
    </GenericTile>
        <content />
    </Page>
</mvc:View>
